	<script>
	  var resp = [
	  	{key:'10096336', value:  6.5 }, {key:'10187710', value:  22.0 }, {key:'10084859', value:  16.5 }, {key:'10278695', value:  16.647058823529413 }, {key:'10267115', value:  4.578947368421052 }, {key:'10366506', value:  4.428571428571429}
	  ];	 

function getRandomColor() {
    var letters = '0123456789ABCDEF';
    var color = '#';
    for (var i = 0; i < 6; i++ ) {
        color += letters[Math.floor(Math.random() * 16)];
    }
    return color;
}

var svg = d3.select("body").append("svg").attr("width",700).attr("height",300);
svg.append("g").attr("id","queryResult");

Donut3D.draw("queryResult", mapData(), 450, 150, 130, 100, 30, 0);

function mapData(){
	return resp.map(function(resp){ 
		return {label:resp.key, value:resp.value, color:getRandomColor()};});
}
</script>

